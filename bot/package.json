{
  "name": "payment-bot",
  "description": "Bot da SeventyHost, só que agora de código aberto e feito em TypeScript.",
  "version": "2.0.0",
  "main": "dist/app.js",
  "bin": "build/src/app.js",
  "pkg": {
    "scripts": [
      "build/src/**/*.js",
      "build/src/**/*.json",
      "build/package.json"
    ],
    "assets": [
      "build/node_modules/**/*.js",
      "build/node_modules/**/*.cjs",
      "build/node_modules/**/*.json",
      "build/node_modules/**/*.node"
    ],
    "outputPath": "release"
  },
  "prisma": {
    "schema": "src/prisma/schema.prisma"
  },
  "scripts": {
    "start": "node .",
    "start:build": "npm run build && node .",
    "nodemon": "nodemon",
    "devs": "ts-node-dev --respawn --transpile-only --transpiler sucrase/ts-node-plugin -r tsconfig-paths/register src/app.ts",
    "dev": "ts-node -r tsconfig-paths/register --transpiler sucrase/ts-node-plugin src/app.ts",
    "update": "npx npm-check -u",
    "build": "tsc && tscpaths -p tsconfig.json -s ./src -o ./dist",
    "build:test": "tsc --project ./tsconfig.json --declaration true -declarationDir dist/types && tsc-alias",
    "build:release": "ts-node release.ts",
    "lint": "eslint --ext .tsx,.ts --fix ./src -c .eslintrc.json"
  },
  "keywords": [
    "Bot",
    "Payment",
    "Discord",
    "Mercado Pago",
    "MP"
  ],
  "author": "Ashu11-a",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Ashu11-A/PaymentBot.git"
  },
  "bugs": {
    "url": "https://github.com/Ashu11-A/PaymentBot/issues"
  },
  "homepage": "https://github.com/Ashu11-A/PaymentBot#readme",
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/body-parser": "^1.19.5",
    "@types/cli-progress": "^3.11.5",
    "@types/cors": "^2.8.15",
    "@types/crypto-js": "^4.2.2",
    "@types/express": "^4.17.18",
    "@types/mercadopago": "^1.5.9",
    "@types/node": "^20.12.8",
    "@types/randomstring": "^1.1.8",
    "@types/signale": "^1.4.4",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^6.5.0",
    "@typescript-eslint/parser": "^6.5.0",
    "@vercel/ncc": "^0.38.1",
    "@yao-pkg/pkg": "^5.11.2",
    "cli-progress": "^3.12.0",
    "esbuild": "^0.20.2",
    "esbuild-node-externals": "^1.13.0",
    "eslint": "^8.47.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-n": "^16.0.2",
    "eslint-plugin-promise": "^6.1.1",
    "javascript-obfuscator": "^4.1.0",
    "sucrase": "^3.34.0",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsc-alias": "^1.8.8",
    "tsconfig-paths": "^4.2.0",
    "tscpaths": "^0.0.9",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@magicyan/discord": "^1.0.3",
    "axios": "^1.5.0",
    "better-sqlite3": "^9.5.0",
    "body-parser": "^1.20.2",
    "chalk": "^4.1.2",
    "cloudflare": "^3.1.0",
    "colors": "^1.4.0",
    "cors": "^2.8.5",
    "cron-parser": "^4.9.0",
    "crypto-js": "^4.2.0",
    "discord.js": "^14.14.1",
    "dotenv": "^16.3.1",
    "eslint": "^8.46.0",
    "express": "^4.18.2",
    "glob": "^10.3.3",
    "helmet": "^7.0.0",
    "loggings": "^1.1.0",
    "mercadopago": "^2.0.9",
    "prisma": "^5.13.0",
    "quick.db": "^9.1.7",
    "randomstring": "^1.3.0",
    "signale": "^1.4.0",
    "terser": "^5.30.4",
    "uuid": "^9.0.1"
  }
}
