{
    "env": {
        "node": true,
        "es2021": true,
        "browser": true,
        "commonjs": true
    },
    "parser": "@typescript-eslint/parser",
    "extends": "standard-with-typescript",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module",
        "project": ["./tsconfig.json"]
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        /*
        "quotes": "warn",
        "brace-style": "warn",
        "semi":"warn",
        "camelcase":"warn",
        "no-duplicate-imports":"warn",
        "no-extra-semi":"warn",
        */
        "no-new": "off",
        "@typescript-eslint/naming-convention": "off"
    }
}
