{
  "name": "payment-bot",
  "description": "Bot da SeventyHost, só que agora de código aberto e feito em TypeScript.",
  "version": "2.0.0",
  "main": "dist/app.js",
  "bin": "build/app.js",
  "pkg": {
    "scripts": [
      "build/**/*.js",
      "package.json"
    ],
    "assets": [
      "node_modules/**/*.js",
      "node_modules/**/*.cjs",
      "node_modules/**/*.json",
      "node_modules/**/*.node"
    ],
    "outputPath": "release"
  },
  "scripts": {
    "start": "node .",
    "start:build": "npm run build && node .",
    "nodemon": "nodemon",
    "devs": "ts-node-dev --respawn --transpile-only --transpiler sucrase/ts-node-plugin -r tsconfig-paths/register src/app.ts",
    "dev": "ts-node -r tsconfig-paths/register --transpiler sucrase/ts-node-plugin src/app.ts",
    "build": "tsc && tscpaths -p tsconfig.json -s ./src -o ./dist",
    "build:test": "tsc --project ./tsconfig.json --declaration true -declarationDir dist/types && tsc-alias",
    "build:release": "npx ncu -i && npm run build && ts-node release.ts",
    "build:rollup": "npm run build:test && rollup --config rollup.config.ts --configPlugin @rollup/plugin-typescript --bundleConfigAsCjs",
    "lint": "eslint --ext .tsx,.ts --fix ./src -c .eslintrc.json"
  },
  "keywords": [
    "Bot",
    "Payment",
    "Discord",
    "Mercado Pago",
    "MP"
  ],
  "author": "Ashu11-a",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Ashu11-A/PaymentBot.git"
  },
  "bugs": {
    "url": "https://github.com/Ashu11-A/PaymentBot/issues"
  },
  "homepage": "https://github.com/Ashu11-A/PaymentBot#readme",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-multi-entry": "^6.0.1",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@types/axios": "^0.14.0",
    "@types/cli-progress": "^3.11.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/mercadopago": "^1.5.11",
    "@types/node": "^20.12.7",
    "@types/randomstring": "^1.3.0",
    "@types/signale": "^1.4.7",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^6.5.0",
    "@typescript-eslint/parser": "^6.5.0",
    "cli-progress": "^3.12.0",
    "eslint": "^8.47.0",
    "eslint-config-standard-with-typescript": "^38.0.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^16.0.0",
    "eslint-plugin-promise": "^6.1.1",
    "rollup-plugin-tsconfig-paths": "^1.5.2",
    "sucrase": "^3.35.0",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.8",
    "tsconfig-paths": "^4.2.0",
    "tscpaths": "^0.0.9",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "@magicyan/discord": "^1.0.30",
    "@typescript-eslint/parser": "^6.5.0",
    "@yao-pkg/pkg": "^5.11.5",
    "axios": "^1.6.8",
    "better-sqlite3": "^9.5.0",
    "body-parser": "^1.20.2",
    "chalk": "^5.3.0",
    "colors": "^1.4.0",
    "cors": "^2.8.5",
    "cron-parser": "^4.9.0",
    "discord.js": "^14.14.1",
    "dotenv": "^16.4.5",
    "eslint": "^9.0.0",
    "express": "^4.19.2",
    "glob": "^10.3.12",
    "helmet": "^7.1.0",
    "loggings": "^1.1.0",
    "mercadopago": "^2.0.8",
    "quick.db": "^9.1.7",
    "randomstring": "^1.3.0",
    "rollup": "^4.14.1",
    "rollup-plugin-polyfill-node": "^0.13.0",
    "signale": "^1.4.0",
    "terser": "^5.30.3",
    "ts-node-dev": "^2.0.0",
    "uuid": "^9.0.1"
  }
}
